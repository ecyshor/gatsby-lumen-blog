{
    "name": "gatsby-remark-embed-url",
    "description": "Gatsby plugin to embed script output",
    "version": "0.1.0",
    "author": "Nicu Reut <contact@nicu.dev>",
    "dependencies": {
        "@babel/runtime": "^7.12.5",
        "lz-string": "^1.4.4",
        "normalize-path": "^3.0.0",
        "npm-package-arg": "^6.1.1",
        "recursive-readdir": "^2.2.2",
        "unist-util-map": "^1.0.5",
        "urijs": "^1.19.2",
        "node-fetch": "^2.6.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.1",
        "@babel/core": "^7.12.3",
        "babel-preset-gatsby-package": "^0.9.0-next.0",
        "cross-env": "^7.0.2"
    },
    "main": "index.js",
    "peerDependencies": {
        "gatsby": "^2.0.0"
    },
    "scripts": {
        "build": "babel src --out-dir . --ignore \"**/__tests__\"",
        "prepare": "cross-env NODE_ENV=production npm run build",
        "watch": "babel -w src --out-dir . --ignore \"**/__tests__\""
    },
    "engines": {
        "node": ">=10.13.0"
    }
}